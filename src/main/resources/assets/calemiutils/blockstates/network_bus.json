{
  "multipart": [
    {
      "apply": {
        "model": "calemiutils:network_bus_core"
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "calemiutils:network_cable_connection",
        "x": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "calemiutils:network_cable_connection",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "calemiutils:network_cable_connection",
        "y": 270,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "calemiutils:network_cable_connection",
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "calemiutils:network_cable_connection",
        "y": 90,
        "uvlock": false
      }
    }
  ]
}
